<query id="getExtraMenuModuleSrls" action="select">
    <tables>
        <table name="modules" alias="modules" />
    </tables>
    <columns>
        <column name="modules.module_srl" alias="module_srl" />
    </columns>
    <conditions>
        <condition operation="equal" column="modules.site_srl" var="site_srl" notnull="notnull" filter="number" />
	</conditions>
</query>
