<query id="updateUpgletyleInfo" action="update">
    <tables>
        <table name="upgletyle" />
    </tables>
	<columns>
		<column name="upgletyle_content" var="upgletyle_content" default="" />
        <column name="upgletyle_title" var="upgletyle_title" default="" />
		<column name="timezone" var="timezone" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
