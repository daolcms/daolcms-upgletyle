<query id="getBlogApiServices" action="select">
    <tables>
        <table name="textyle_blogapi_services" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="textyle_blogapi_services_srl" var="textyle_blogapi_services_srl" filter="number" />
    </conditions>
    <navigation>
		<index var="list_order" default="list_order" order="asc" />
    </navigation>
</query>
