        <node target="mid" required="true" filter="alpha_number" />
        <node target="post_style" required="true" />
        <node target="post_list_count" required="true" />
        <node target="category_list_count" required="true" />
        <node target="rss_list_count" required="true" />
        <node target="commnet_list_count" required="true" />
        <node target="guestbook_list_count" required="true" />
        <node target="rss_type" required="true" />
        <node target="input_email" required="true" />
        <node target="input_website" required="true" />
        <node target="post_use_prefix" required="true" />
        <node target="post_use_suffix" required="true" />
        <node target="post_prefix" required="true" />
        <node target="post_suffix" required="true" />
        <node target="ping_back"  />
        <node target="twitter_consumer_key"  />
        <node target="twitter_consumer_secret"  />
        <node target="twitter_oauth_token"  />
        <node target="twitter_oauth_token_secret"  />
        <node target="enable_twitter"  />
        <node target="twitter_userid"  />
        <node target="twitter_password"  />
        <node target="comment_grant"  />
        <node target="guestbook_grant"  />
